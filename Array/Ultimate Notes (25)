  if(array_nums[i] > max)
			max = array_nums[i];
		else if(array_nums[i] < min)
			min = array_nums[i];
      
  29. Check array without 0 and -1
  for (int number : numbers) {
      if (number == 0 || number == -1) {
        return false;
      }
    }
    
    
      
