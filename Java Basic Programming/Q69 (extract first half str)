Pyt

// <SAMPLE SOLUTION>
import java.lang.*;
 public class Exercise69 {
 public static void main(String[] args)
 {
    String main_string = "Python";    
    System.out.println(main_string.substring(0, main_string.length()/2));	
  } 
 }
