	x = n / 10;		//x=24/10 = 2
	y = n - x *10;		//y=24-2*10 = 4
	System.out.println("In English: "+number[x] + " " + number[y]);
}
else {			//3 digits
	x = n / 100;			
	y = (n - x * 100) / 10;
	z = n - x * 100 - y * 10;
	System.out.println("In English: "+number[x] + " " + number[y] + " " + number[z]);
}

